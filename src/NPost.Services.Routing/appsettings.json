{
  "app": {
    "name": "NPost Routing Service"
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "routing-service",
    "address": "docker.for.win.localhost",
    "port": "5003",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "routing-service"
  },
  "httpClient": {
    "type": "fabio",
    "retries": 2,
    "services": {
    }
  },
  "logger": {
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    }
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "API",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true
  }
}
